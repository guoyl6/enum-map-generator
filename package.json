{
  "name": "enum-map-generator",
  "displayName": "Enum Map Generator",
  "description": "Generate mapper for enum in TS/CS",
  "version": "0.0.1",
  "icon": "assets/icon.png",
  "author": {
    "email": "nurulhudaapon@gmail.com",
    "name": "Nurul Huda"
  },
  "publisher": "nurulhudaapon",
  "engines": {
    "vscode": "^1.56.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nurulhudaapon/enum-map-generator.git"
},
  "categories": [
    "Programming Languages",
    "Snippets",
    "Formatters"
  ],
  "activationEvents": [
    "onCommand:enum-map-generator.paste_mapped_enum"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "keybindings": [
      {
        "command": "enum-map-generator.paste_mapped_enum",
        "win": "shift+alt+m",
        "mac": "shift+alt+v",
        "linux": "shift+alt+m"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "enum-map-generator.paste_mapped_enum",
          "group": "9_cutcopypaste"
        }
      ]
    },
    "commands": [
      {
        "command": "enum-map-generator.paste_mapped_enum",
        "title": "Enum Mapper (Paste Mapping)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "npm run test-compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0"
  }
}